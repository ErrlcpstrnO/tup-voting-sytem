    "@types/express-session": "^1.17.4",
    "@types/express-validator": "^3.0.0",
    "@types/flat": "^5.0.2",
    "@types/json2csv": "^5.0.3",
    "@types/morgan": "^1.9.3",
    "@types/node": "^8.0.29",
    "@types/nodemailer": "^6.4.4",
    "@types/nodemailer-express-handlebars": "^4.0.2",
