{
  "name": "tup-voting-system-server",
  "version": "1.0.0",
  "description": "voting system for technological university of the philippines",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist/",
    "copy-files": "copyfiles -u 1  \"src/templates/**\" \"dist/\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "typeorm": "set NODE_ENV=development&&node --require ts-node/register ./node_modules/typeorm/cli.js --config src/ormconfig",
    "build": "yarn clean && tsc && yarn copy-files",
    "start": "set NODE_ENV=production&& node dist/index.js ",
    "dev": "set NODE_ENV=development&set DEBUG=express-session& nodemon src/index.ts",
    "prod": "set NODE_ENV=production&& node build/index",
    "start:dev": "set NODE_ENV=development&& nodemon src/index.ts",
    "start:prod": "set NODE_ENV=production&& tsc && node dist/index.js",
    "deploy:dev": "npm run start:dev",
    "deploy:prod": "npm run start:prod"
  },
  "author": "dev404",
  "dependencies": {
    "axios": "^0.26.1",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "class-validator": "^0.13.2",
    "cloudinary": "^1.27.1",
    "connect-flash": "^0.1.1",
    "connect-typeorm": "^1.1.4",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "csvtojson": "^2.0.10",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "express-session": "^1.17.2",
    "express-validator": "^6.13.0",
    "flat": "^5.0.2",
    "json2csv": "^5.0.6",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "nanoid": "^3.3.2",
    "neat-csv": "^7.0.0",
    "nodemailer": "^6.7.3",
    "nodemailer-express-handlebars": "^5.0.0",
    "npm": "^8.5.5",
    "papaparse": "^5.3.2",
    "passport": "^0.5.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.4.0",
    "reflect-metadata": "^0.1.10",
    "typeorm": "^0.2.41",
    "typeorm-naming-strategies": "^2.0.0"
